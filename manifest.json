{
  "manifest_version": 3,
  "name": "Waseda Grade Exporter",
  "version": "1.0",
  "description": "成績一覧をcsv形式で出力します。",
  "permissions": ["activeTab", "scripting", "contextMenus"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://gradereport-ty.waseda.jp/kyomu/*"],
      "js": ["content.js"]
    }
  ]
}
